{
  "expo": {
    "name": "MyBuy",
    "slug": "MyBuy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icone.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/splash_screen.png",
        "resizeMode": "contain",
        "backgroundColor": "#b759f3"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icone.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.erickmota.MyBuy",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3c2be47a-e524-47c5-903f-dd37ec600d07"
      }
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
